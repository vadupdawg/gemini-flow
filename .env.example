# Gemini API Key (required)
GEMINI_API_KEY=your-gemini-api-key-here

# Optional: Claude API Key for MCP integration
CLAUDE_API_KEY=your-claude-api-key-here

# Optional: Disable parallel execution to avoid worker errors
DISABLE_PARALLEL_EXECUTION=false

# Optional: Max parallel workers (default: 4)
MAX_PARALLEL_WORKERS=4

# Optional: Debug mode
DEBUG=false